---
import BaseLayout from '@layouts/BaseLayout.astro'
import ListExperience from '@components/ListExperience.astro';
import { getPostsByLocale } from '@utils/paths';
const allPosts = await getPostsByLocale('community', 'es');
const pageTitle = "Comunidad";
---
<BaseLayout pageTitle={pageTitle}>
  <ListExperience posts={allPosts} basePath='es/comunidad' />
</BaseLayout>
